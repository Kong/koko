{
  "required": [
    "id",
    "kid"
  ],
  "properties": {
    "created_at": {
      "minimum": 1,
      "type": "integer"
    },
    "id": {
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "type": "string",
      "description": "must be a valid UUID"
    },
    "jwk": {
      "type": "string"
    },
    "kid": {
      "type": "string"
    },
    "name": {
      "maxLength": 128,
      "minLength": 1,
      "pattern": "^[0-9a-zA-Z.\\-_~]*$",
      "type": "string"
    },
    "pem": {
      "required": [
        "private_key"
      ],
      "properties": {
        "private_key": {
          "format": "pem-encoded-private-key"
        },
        "public_key": {
          "format": "pem-encoded-public-key"
        }
      },
      "type": "object"
    },
    "set": {
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
          "type": "string",
          "description": "must be a valid UUID"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "tags": {
      "items": {
        "maxLength": 128,
        "minLength": 1,
        "pattern": "^(?:[0-9a-zA-Z.\\-_~:]+(?: *[0-9a-zA-Z.\\-_~:])*)?$",
        "type": "string"
      },
      "maxItems": 8,
      "uniqueItems": true,
      "type": "array"
    },
    "updated_at": {
      "minimum": 1,
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "type": "object",
  "allOf": [
    {
      "oneOf": [
        {
          "required": [
            "jwk"
          ],
          "properties": {
            "pem": {
              "not": {
                "description": "there's a JWK, don't set PEM"
              }
            }
          }
        },
        {
          "required": [
            "pem"
          ],
          "properties": {
            "jwk": {
              "not": {
                "description": "there's a PEM, don't set JWK"
              }
            }
          }
        }
      ],
      "title": "one key format",
      "description": "Keys must be defined either in JWK or PEM format"
    }
  ]
}