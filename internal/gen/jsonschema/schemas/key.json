{
  "required": [
    "id",
    "kid"
  ],
  "properties": {
    "created_at": {
      "minimum": 1,
      "type": "integer"
    },
    "id": {
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "type": "string",
      "description": "must be a valid UUID"
    },
    "jwk": {
      "type": "string"
    },
    "kid": {
      "type": "string"
    },
    "name": {
      "maxLength": 128,
      "minLength": 1,
      "pattern": "^[0-9a-zA-Z.\\-_~]*$",
      "type": "string"
    },
    "pem": {
      "type": "string"
    },
    "set": {
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
          "type": "string",
          "description": "must be a valid UUID"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "tags": {
      "items": {
        "maxLength": 128,
        "minLength": 1,
        "pattern": "^(?:[0-9a-zA-Z.\\-_~:]+(?: *[0-9a-zA-Z.\\-_~:])*)?$",
        "type": "string"
      },
      "maxItems": 8,
      "uniqueItems": true,
      "type": "array"
    },
    "updated_at": {
      "minimum": 1,
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "type": "object"
}