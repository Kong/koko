{
  "required": [
    "name"
  ],
  "properties": {
    "config": {
      "additionalProperties": true,
      "type": "object"
    },
    "consumer": {
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
          "type": "string",
          "description": "must be a valid UUID"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "created_at": {
      "minimum": 1,
      "type": "integer"
    },
    "enabled": {
      "type": "boolean"
    },
    "id": {
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "type": "string",
      "description": "must be a valid UUID"
    },
    "name": {
      "maxLength": 128,
      "minLength": 1,
      "pattern": "^[0-9a-zA-Z\\-]*$",
      "type": "string"
    },
    "ordering": {
      "additionalProperties": true,
      "type": "object"
    },
    "protocols": {
      "items": {
        "enum": [
          "http",
          "https",
          "grpc",
          "grpcs",
          "tcp",
          "udp",
          "tls",
          "tls_passthrough"
        ],
        "type": "string"
      },
      "maxItems": 8,
      "type": "array"
    },
    "route": {
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
          "type": "string",
          "description": "must be a valid UUID"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "service": {
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
          "type": "string",
          "description": "must be a valid UUID"
        }
      },
      "additionalProperties": false,
      "type": "object"
    },
    "tags": {
      "items": {
        "maxLength": 128,
        "minLength": 1,
        "pattern": "^[0-9a-zA-Z.\\-_~:]*$",
        "type": "string"
      },
      "maxItems": 8,
      "uniqueItems": true,
      "type": "array"
    },
    "updated_at": {
      "minimum": 1,
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "type": "object",
  "allOf": [
    {
      "not": {
        "required": [
          "ordering"
        ]
      },
      "title": "ordering",
      "description": "'ordering' is a Kong Enterprise only feature. Please contact your administrator to upgrade to Enterprise and start using the 'ordering' field"
    }
  ],
  "x-koko-config": {
    "disableValidateEndpoint": false,
    "resourceAPIPath": "plugins"
  }
}