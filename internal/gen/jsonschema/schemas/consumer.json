{
  "required": [
    "id"
  ],
  "properties": {
    "created_at": {
      "minimum": 1,
      "type": "integer"
    },
    "custom_id": {
      "maxLength": 128,
      "minLength": 1,
      "pattern": "^[0-9a-zA-Z.\\-_~]+(?: [0-9a-zA-Z.\\-_~]+)*$",
      "type": "string"
    },
    "id": {
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "type": "string",
      "description": "must be a valid UUID"
    },
    "tags": {
      "items": {
        "maxLength": 128,
        "minLength": 1,
        "pattern": "^[0-9a-zA-Z.\\-_~:]*$",
        "type": "string"
      },
      "maxItems": 8,
      "uniqueItems": true,
      "type": "array"
    },
    "updated_at": {
      "minimum": 1,
      "type": "integer"
    },
    "username": {
      "maxLength": 128,
      "minLength": 1,
      "pattern": "^[0-9a-zA-Z.\\-_~+@]*$",
      "type": "string"
    }
  },
  "additionalProperties": false,
  "type": "object",
  "anyOf": [
    {
      "required": [
        "custom_id"
      ],
      "description": "at least one of custom_id or username must be set"
    },
    {
      "required": [
        "username"
      ],
      "description": "at least one of custom_id or username must be set"
    }
  ]
}