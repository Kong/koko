{
  "required": [
    "id",
    "cert",
    "key"
  ],
  "properties": {
    "cert": {
      "type": "string",
      "format": "pem-encoded-cert"
    },
    "cert_alt": {
      "type": "string",
      "format": "pem-encoded-cert"
    },
    "created_at": {
      "minimum": 1,
      "type": "integer"
    },
    "id": {
      "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
      "type": "string",
      "description": "must be a valid UUID"
    },
    "key": {
      "type": "string",
      "format": "pem-encoded-private-key"
    },
    "key_alt": {
      "type": "string",
      "format": "pem-encoded-private-key"
    },
    "tags": {
      "items": {
        "maxLength": 128,
        "minLength": 1,
        "pattern": "^[0-9a-zA-Z.\\-_~:]*$",
        "type": "string"
      },
      "maxItems": 8,
      "type": "array"
    },
    "updated_at": {
      "minimum": 1,
      "type": "integer"
    }
  },
  "dependencies": {
    "cert_alt": {
      "required": [
        "key_alt"
      ]
    },
    "key_alt": {
      "required": [
        "cert_alt"
      ]
    }
  }
}